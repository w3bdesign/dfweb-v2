{
  "name": "dfweb",
  "private": true,
  "description": "Personal portfolio",
  "version": "1.0.30",
  "license": "MIT",
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "develop": "gatsby clean && gatsby develop",
    "dev": "start-test develop 8000 cy:open",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test:cypress": "cypress run",
    "test": "jest --env=jsdom",
    "testcafe": "start-test develop http://localhost:8000 testcafex",
    "testcafex": "percy exec -- testcafe chrome:headless testcafe/**/*",
    "cy:open": "cypress open",
    "stylelint": "stylelint **/*.css --fix",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm i --legacy-peer-deps",
    "bump": "npx npm-check-updates --target minor -u && npm i",
    "lint": "eslint --fix --ext .js,.jsx ."
  },
  "dependencies": {
    "@emailjs/browser": "^3.4.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@types/cypress": "^1.1.3",
    "babel-jest": "^27.5.1",
    "babel-plugin-styled-components": "^2.0.2",
    "dotenv": "^16.0.0",
    "gatsby": "^4.13.1",
    "gatsby-plugin-image": "^2.13.0",
    "gatsby-plugin-manifest": "^4.13.0",
    "gatsby-plugin-postcss": "^5.13.0",
    "gatsby-plugin-react-helmet": "^5.13.0",
    "gatsby-plugin-react-svg": "^3.1.0",
    "gatsby-plugin-robots-txt": "^1.7.1",
    "gatsby-plugin-sharp": "^4.13.0",
    "gatsby-plugin-sitemap": "^5.13.0",
    "gatsby-plugin-styled-components": "^5.13.0",
    "gatsby-source-filesystem": "^4.13.0",
    "gatsby-transformer-sharp": "^4.13.0",
    "netlify-plugin-cypress": "^2.2.0",
    "node-fetch": "2.6.7",
    "postcss": "^8.4.13",  
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-reveal-effects": "^1.1.2",
    "react-simple-captcha": "^8.2.6",
    "react-spring": "^9.4.5",
    "styled-components": "^5.3.5",
    "tailwindcss": "^3.0.24",
    "testcafe": "^1.18.6",
    "testcafe-react-selectors": "^4.1.5"
  },
  "devDependencies": {
    "@percy/cli": "1.1.2",
    "@percy/testcafe": "^1.0.2",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.1.1",
    "babel-eslint": "^10.1.0",
    "babel-preset-gatsby": "^2.13.0",
    "classnames": "^2.3.1",
    "cypress": "^9.6.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "react-test-renderer": "^17.0.2",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.8.2",
    "stylelint-config-standard": "^25.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  }
}
